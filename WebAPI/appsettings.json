{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Custom": {
	"CorrelationName": "X-Correlation-ID"
  },

  "Serilog": {
	"MinimumLevel": {
	  "Override": {
		"Microsoft.AspNetCore.Hosting": "Warning",
		"Microsoft.AspNetCore.Mvc": "Warning",
		"Microsoft.AspNetCore.Routing": "Warning"
	  }
	}
  }
}
